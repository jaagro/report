<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jaagro.report.biz.mapper.tms.WaybillGoodsMapperExt">
    <select id="listGoodsByWaybillId" resultType="com.jaagro.report.api.dto.waybill.GetWaybillGoodsDto"
            parameterType="java.lang.Integer">
        select * from waybill_goods
        <where>
            enabled=1
            <if test="waybillIds!=null">
                and waybill_id IN
                <foreach collection="waybillIds" open="(" separator="," close=")" item="waybillId">
                    ${waybillId}
                </foreach>
            </if>
        </where>
    </select>
</mapper>