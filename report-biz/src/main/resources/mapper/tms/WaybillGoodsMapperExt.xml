<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jaagro.report.biz.mapper.tms.WaybillGoodsMapperExt">
    <select id="listGoodsByWaybillId" resultType="com.jaagro.report.api.dto.waybill.GetWaybillGoodsDto"
            parameterType="java.lang.Integer">
        select *  from waybill_goods  where waybill_id = #{waybillId} and enabled=1
    </select>
</mapper>