<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jaagro.report.biz.mapper.cbs.BatchInfoMapperExt">

    <select id="findByBatchNoAndDayAge" resultType="integer">
        SELECT
            start_amount
        FROM
            cbsserver.batch_info
        WHERE
            enable = 1
        AND batch_no = #{batchNo}
        AND day_age = #{dayAge}
        ORDER BY
            create_time DESC
        LIMIT 1
    </select>
</mapper>